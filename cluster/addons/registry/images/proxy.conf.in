global
  maxconn 1024
  debug

defaults
  mode http
  log global
  option httplog
  retries 3
  option redispatch
  timeout client 1s
  timeout server 5s
  timeout connect 5s

frontend forwarder
  log global
  bind *:%FWDPORT%
  default_backend registry

backend registry
  server kube-registry %HOST%:%PORT% ssl verify required ca-file %CA_FILE%
